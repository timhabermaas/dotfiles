{
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
      "filetypes": ["haskell", "lhaskell"],
      "initializationOptions": {
        "languageServerHaskell": {
          "formattingProvider": "ormolu"
        }
      }
    }
  },
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "typescriptreact", "javascript", "rust", "css", "haskell"],
  "rust-analyzer.inlayHints.enable": true,
  "rust-analyzer.diagnostics.enable": true,
  "cSpell.userWords": [
    "BIGSERIAL",
    "Deserializable",
    "Fixnum",
    "Mustermann",
    "TIMESTAMPTZ",
    "accummulating",
    "actix",
    "chrono",
    "csrf",
    "ctype",
    "cubemania",
    "dnfs",
    "healthcheck",
    "hmac",
    "isin",
    "itertools",
    "jsonb",
    "jsonwebtoken",
    "ossp",
    "overfetched",
    "overfetching",
    "postgres",
    "postgresql",
    "powf",
    "splitn",
    "sqlx",
    "svgs",
    "thiserror",
    "timespan",
    "varchar"
  ]
}
